   "source": [
    "# Cell 3: Define text cleaning function\n",
    "def clean_text(text):\n",
    "    \"\"\"\n",
    "    改进的文本清洗函数\n",
    "    \"\"\"\n",
    "    if not isinstance(text, str):\n",
    "        return \"\"\n",
    "    \n",
    "    # 1. 移除HTML标签\n",
    "    text = re.sub(r'<.*?>', '', text)\n",
    "    # 2. 移除URL\n",
    "    text = re.sub(r'http\\S+', '', text)\n",
    "    # 3. 移除@提及\n",
    "    text = re.sub(r'@\\w+', '', text)\n",
    "    \n",
    "    # 4. 添加：小写化\n",
    "    text = text.lower()\n",
    "    \n",
    "    # 5. 添加：更彻底的特殊字符处理（您可能缺少的）\n",
    "    # 保留字母、数字、空格和基本标点\n",
    "    text = re.sub(r'[^a-zA-Z0-9\\s\\.,!?]', '', text)\n",
    "    \n",
    "    # 6. 合并多余空格\n",
    "    text = re.sub(r'\\s+', ' ', text).strip()\n",
    "    \n",
    "    return text"
   ]
  },
