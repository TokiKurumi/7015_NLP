    "    print(\"Current working directory:\", os.getcwd())\n",
    "    print(\"Directory contents:\", os.listdir('.'))\n",
    "else:\n",
    "    train_df, test_df = load_imdb_from_local(data_path)\n",
    "    print(\" Data loading completed\")"
   ]
